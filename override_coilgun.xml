<?xml version="1.0" encoding="utf-8"?>
<Override>
  <!-- 标准弹药箱 -->
  <Item identifier="coilgunammobox">
    <inherit identifier="coilgunammobox">
      <doinherit />
      <!-- 按tag取材料 -->
      <del sel="./Fabricate[@displayname='recycleitem']/RequiredItem[@identifier='coilgunammobox']/@identifier" />
      <add sel="./Fabricate[@displayname='recycleitem']/RequiredItem[@usecondition='false']/@tag">coilgunammo</add>
      <!-- 可放快捷栏 -->
      <replace sel="./Holdable/@slots">Any,RightHand+LeftHand</replace>
      <!-- 可合并耐久 -->
      <add sel="./Holdable/@canBeCombined">true</add>
    </inherit>
  </Item>
