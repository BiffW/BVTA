<?xml version="1.0" encoding="utf-8"?>
<Override>
  <!-- 标准炮弹 -->
  <Item identifier="railgunshell">
    <inherit identifier="railgunshell">
      <doinherit />
      <replace sel="./Price/@baseprice">55</replace>
      <del sel="./Deconstruct/Item" />
      <add sel="./Deconstruct">
        <Item identifier="iron" />
        <Item identifier="carbon" />
      </add>
      <del sel="./Fabricate/RequiredItem[@identifier='iron']" />
      <replace sel="./Projectile/Attack/@severlimbsprobability">50</replace>
      <replace sel="./Projectile/Attack/Affliction[@identifier='explosiondamage']/@strength">500</replace>
      <add sel="./Projectile/Attack">
        <Affliction identifier="bleeding" strength="6" multiplybymaxvitality="true" />
      </add>
      <replace sel="./Projectile/StatusEffect[@type='OnImpact']/Explosion/@severlimbsprobability">5</replace>
      <replace sel="./ItemContainer/@capacity">2</replace>
    </inherit>
  </Item>
  <!-- 核弹 -->
  <Item identifier="nuclearshell">
    <inherit identifier="nuclearshell">
      <doinherit />
      <replace sel="./Price/Price[@minavailable='1']/@minavailable">3</replace>
      <replace sel="./Projectile/Attack/@severlimbsprobability">50</replace>
      <replace sel="./Projectile/Attack/Affliction[@identifier='explosiondamage']/@strength">500</replace>
      <add sel="./Projectile/Attack">
        <Affliction identifier="bleeding" strength="6" multiplybymaxvitality="true" />
      </add>
      <replace sel="./Projectile/StatusEffect[@type='OnImpact']/Explosion/@severlimbsprobability">20</replace>
      <add sel="./Projectile/StatusEffect[@type='OnImpact']/Explosion">
        <Affliction identifier="explosiondamage" strength="15" multiplybymaxvitality="true" />
        <Affliction identifier="burn" strength="10" multiplybymaxvitality="true" />
      </add>
      <replace sel="./ItemContainer/@capacity">2</replace>
    </inherit>
  </Item>
  <!-- 刚素弹 -->
  <Item identifier="alienshell">
    <inherit identifier="alienshell">
      <doinherit />
      <replace sel="./Price/@baseprice">330</replace>
      <del sel="./Price/@sold" />
      <add sel="./Price/@minleveldifficulty">35</add>
      <del sel="./Price/Price" />
      <add sel="./Price">
        <Price storeidentifier="merchantoutpost" multiplier="1.5" sold="false" />
        <Price storeidentifier="merchantcity" multiplier="1.25" sold="false" />
        <Price storeidentifier="merchantresearch" multiplier="1.25" sold="false" />
        <Price storeidentifier="merchantmilitary" multiplier="0.9" minavailable="3" />
        <Price storeidentifier="merchantmine" multiplier="1.0" minavailable="3" />
        <Price storeidentifier="merchantarmory" multiplier="0.9" minavailable="3" />
      </add>
      <add sel="./Deconstruct">
        <Item identifier="titanium" />
        <Item identifier="aluminium" />
      </add>
      <replace sel="./Fabricate/RequiredItem[@identifier='titanium']/@identifier">titaniumaluminiumalloy</replace>
      <replace sel="./Projectile/Attack/@severlimbsprobability">200</replace>
      <replace sel="./Projectile/Attack/Affliction[@identifier='explosiondamage']/@strength">1562.5</replace>
      <add sel="./Projectile/Attack">
        <Affliction identifier="bleeding" strength="24" multiplybymaxvitality="true" />
      </add>
      <replace sel="./Projectile/StatusEffect[@type='OnImpact']/Explosion/@severlimbsprobability">5</replace>
      <replace sel="./ItemContainer/@capacity">2</replace>
    </inherit>
  </Item>
  <!-- 霰弹 -->
  <Item identifier="railguncanistershell">
    <inherit identifier="railguncanistershell">
      <doinherit />
      <replace sel="./Price/@baseprice">59</replace>
      <add sel="./Price/@minavailable">3</add>
      <del sel="./Price/Price" />
      <add sel="./Price">
        <Price storeidentifier="merchantoutpost" multiplier="1.2" />
        <Price storeidentifier="merchantcity" multiplier="1.2" minavailable="1" sold="false" />
        <Price storeidentifier="merchantresearch" multiplier="1.2" />
        <Price storeidentifier="merchantmilitary" multiplier="0.9" minavailable="5" />
        <Price storeidentifier="merchantmine" multiplier="1.0" minavailable="5" />
        <Price storeidentifier="merchantarmory" multiplier="0.9" minavailable="5" />
      </add>
      <replace sel="./Deconstruct/Item[@identifier='steel']">
        <Item identifier="iron" />
        <Item identifier="carbon" />
      </replace>
      <del sel="./Fabricate/RequiredItem/@amount" />
      <replace sel="./ItemContainer/@capacity">24</replace>
      <replace sel="./ItemContainer/@spawnwithid">railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet,railguncanistershellpellet</replace>
    </inherit>
  </Item>
</Override>